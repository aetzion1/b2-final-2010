<%# QUESTION: WHY DOES THIS FILE AND DIRECTORY NOT NEED TO BE NESTED WITHIN COMPETITIONS?%>
<h1>Register a New Team</h1>
<%= form_with(model: @team, url: competition_teams_path(params[:competition_id]), method: "post", local: true) do |f| %>
  <%= f.label :hometown %>
  <%= f.text_field :hometown %>
  <%= f.label :nickname %>
  <%= f.text_field :nickname %>

  <%= f.submit 'Register Team' %>
<% end %>